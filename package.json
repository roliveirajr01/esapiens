{
  "name": "teste-esapiens",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently -n VITE,JSON-SERVER \"npm run start\" \"npm run server\"",
    "start": "vite --port 3000",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --report-unused-disable-directives",
    "lint:fix": "npm run lint -- --fix",
    "server": "json-server --watch db.json --port 3001",
    "preview": "concurrently -n \"PREVIEW,API\" \"vite preview\" \"npm run server\"",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal public/vite.svg",
    "prepare": "husky install",
    "precheck": "npm run lint",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "axios": "^1.8.4",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vite-pwa/assets-generator": "^0.0.10",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-import-meta": "^2.3.2",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-server": "^0.17.4",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.16",
    "vite": "^5.4.15",
    "vite-plugin-pwa": "^0.16.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}